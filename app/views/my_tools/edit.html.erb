My Tool
< div class=‘container’>
<%= form_for(@my_tools) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :daily_price %>
  <%= f.number_field :daily_price %>

  <%= f.submit %>
<% end %>
