<h1>Editing My Tool</h1>

<div class=‘container’>
  <%= simple_form_for @my_tool, url: my_tool_path(@my_tool), method: :patch do |f| %>
    <%= f.input :name %>
    <%= f.input :location %>
    <%= f.input :description %>
    <%= f.input :daily_price %>
    <%= f.input :photo %>

    <%= f.submit %>
  <% end %>
</div>

